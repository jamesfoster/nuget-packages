<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>gource</id>
    <title>gource</title>
    <version>0.39</version>
    <authors>Andrew Caudwell</authors>
    <owners>James Foster</owners>
    <summary>Gource is a software version control visualization tool.</summary>
    <description>
      Software projects are displayed by Gource as an animated tree with the root directory of the project at its centre. Directories appear as branches with files as leaves. Developers can be seen working on the tree at the times they contributed to the project.
    </description>
    <projectUrl>https://code.google.com/p/gource/</projectUrl>
    <tags>gource vcs version control visualisation</tags>
    <copyright></copyright>
    <licenseUrl>http://www.gnu.org/licenses/gpl.html</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <releaseNotes></releaseNotes>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>